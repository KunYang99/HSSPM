<div class="header-green">
  <h3><i class='glyphicon glyphicon-list-alt'></i> Search Results</h3>
</div>

<%= form_tag({:action => "search_results"}, :method => 'get') do %>
  <%= select_tag :key, options_for_select(@ops, 'Subject ID') %> 
  <%= text_field_tag :value %>
  <%= submit_tag "Search Again", :class => 'btn btn-primary' %>
<% end %>
<hr>

<%= render partial: "search_table", locals: {dataset_list: @datasets } %> 
