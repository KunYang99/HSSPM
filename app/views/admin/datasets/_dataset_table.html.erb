<%= will_paginate dataset_list, :class => 'digg_pagination' %>
<br>
<div class="table-responsive">
  <table class="table table-bordered table-striped table-condensed">
    <tr>
      <th>Subject ID</th>
      <th>Title</th>
      <th>File</th>
      <th>Availability</th>
      <th>PI</th>
      <th>Contributors</th>
      <th>Projects</th>
      <th>Note</th>
      <th>&nbsp;</th>
    </tr>

    <% dataset_list.each do |s| %>
      <tr>
        <td><%= s.human %></td>
        <td><%= s.title %></td>
        <td><%= link_to s.file_identifier, s.file.url %></td>
        <td><%= s.availability %></td>
        <td><%= s.pi %></td>
        <td><%= s.contributors %></td>
        <td><%= s.to_projects %></td>
        <td><%= s.note %></td>
        <td>
          <%= link_to edit_admin_dataset_path(s) do %><span class='glyphicon glyphicon-pencil'></span><% end %>
          <%= link_to admin_dataset_path(s), :data => {:confirm => 'Want to delete this dataset?'}, :method => :delete do %><span class='glyphicon glyphicon-trash'></span><% end %>
        </td>
      </tr>
    <% end %>
  </table>
</div>

